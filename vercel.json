{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/webshop.html",
      "destination": "/webshop.html"
    },
    {
      "source": "/boeken.html",
      "destination": "/boeken.html"
    },
    {
      "source": "/tarieven.html",
      "destination": "/tarieven.html"
    },
    {
      "source": "/checkout.html",
      "destination": "/checkout.html"
    },
    {
      "source": "/order-bevestiging.html",
      "destination": "/order-bevestiging.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
